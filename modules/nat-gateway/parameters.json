{
    "$schema": "http://json-schema.org/draft-07/schema",
    "definitions": {
        "nat-gateway": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "value": {
                    "$ref": "#/definitions/nat-gateway-parameters"
                }
            },
            "required": [
                "value"
            ]
        },
        "nat-gateways":{
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "value": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/nat-gateway-parameters"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "value"
            ]
        },
        "nat-gateway-parameters": {
            "type": "object",
            "properties": {
                "natGatewayName": {
                    "type": "string"
                },
                "natGatewayResourceGroup": {
                    "type": "string"
                },
                "natGatewayResourceLocation": {
                    "type": "string"
                },
                "natGatewayPublicIpAddresses": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "publicIpName": {
                                "type": "string"
                            },
                            "publicIpResourceGroup": {
                                "type": "string"
                            }
                        }
                    },
                    "minItems": 1
                },
                "natGatewayPublicIpPrefixes": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "publicIpPrefixName": {
                                "type": "string"
                            },
                            "publicIpPrefixResourceGroup": {
                                "type": "string"
                            }
                        }
                    },
                    "minItems": 1
                },
                "natGatewayTags": {
                    "type": "object",
                    "patternProperties": {
                        "^[a-z]": {
                            "type": "string"
                        }
                    },
                    "minProperties": 1
                }
            }
        }
    }
}