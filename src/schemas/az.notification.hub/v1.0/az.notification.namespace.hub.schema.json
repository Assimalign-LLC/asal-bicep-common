{
    "$schema": "http://json-schema.org/draft-07/schema",
    "definitions": {
        "notificationNamespaceHubs": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "The name of the Notification Namespace Hub"
                    },
                    "namespace": {
                        "type": "string",
                        "description": "The name of the notification Namespace."
                    }
                },
                "required": [
                    "name",
                    "namespace"
                ]
            }
        },
        "notificationNamespaceHub": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "The name of the Notification Namespace Hub"
                }
            },
            "required": [
                "name"
            ]
        }
    }
}