{
    /*
        storageAccountTableServices
            - For deploying 
    
    
    */
    "$schema": "http://json-schema.org/draft-07/schema",
    "definitions": {
        "storageAccountTableServices": {
            "type": "object",
            "properties": {
                "value": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "default": "default"
                            },
                            "storageAccountName": {
                                "type": "string"
                            },
                            "storageAccountResourceGroup": {
                                "type": "string"
                            },
                            "tables": {
                                "type": "array",
                                "items": {
                                    "$ref": "#definitions/storageAccountTableServiceTable"
                                }
                            },
                            "privateEndpoint": {
                                "$ref": "#definitions/storageAccountTableServicePrivateEndpoint"
                            }
                        },
                        "required": [
                            "name",
                            "storageAccountName",
                            "storageAccountResourceGroup",
                            "privateEndpoint",
                            "tables"
                        ]
                    }
                }
            }
        },
        "storageAccountTableService": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "default": "default"
                },
                "tables": {
                    "type": "array",
                    "items": {
                        "$ref": "#definitions/storageAccountTableServiceTable"
                    }
                },
                "privateEndpoint": {
                    "$ref": "#definitions/storageAccountTableServicePrivateEndpoint"
                }
            },
            "required": [
                "name",
                "privateEndpoint",
                "tables"
            ]
        },
        "storageAccountTableServiceTable": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "name"
            ]
        },
        "storageAccountTableServiceTables": {
            "type": "object",
            "properties": {
                "value": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "storageAccountTableServiceName": {
                                "type": "string",
                                "default": "default"
                            },
                            "storageAccountName": {
                                "type": "string"
                            },
                            "storageAccountResourceGroup": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "name",
                            "storageAccountTableServiceName",
                            "storageAccountName",
                            "storageAccountResourceGroup"
                        ]
                    }
                }
            },
            "required": [
                "value"
            ]
        },
        "storageAccountTableServicePrivateEndpoint": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "privateDnsZone": {
                            "type": "string"
                        },
                        "privateDnsZoneResourceGroup": {
                            "type": "string",
                            "description": ""
                        },
                        "virtualNetwork": {
                            "type": "string"
                        },
                        "virtualNetworkSubnet": {
                            "type": "string"
                        },
                        "virtualNetworkResourceGroup": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "name",
                        "privateDnsZone",
                        "privateDnsZoneResourceGroup",
                        "virtualNetwork",
                        "virtualNetworkSubnet",
                        "virtualNetworkResourceGroup"
                    ]
                },
                {
                    "type": "null"
                }
            ]
        }
    }
}